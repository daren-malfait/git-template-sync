    "url": "https://github.com/daren-malfait/git-template-sync"
  },
  "bugs": {
    "url": "https://github.com/daren-malfait/git-template-sync/issues"
  },
  "homepage": "https://github.com/daren-malfait/git-template-sync#readme",
  "files": [
    "dist"
